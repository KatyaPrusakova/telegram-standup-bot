{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev &",
    "local": "npm-run-all -n -s dev ngrok webhook whinfo",
    "ngrok": "ngrok http 3000 &",
    "webhook": "curl -vvv \"https://api.telegram.org/bot$(dotenv -e .env.local -p TELEGRAM_API_KEY)/setWebhook?url=$(curl -s localhost:4040/api/tunnels | jq -r \".tunnels[0].public_url\")/api/standup?key=$(dotenv -e .env.local -p TELEGRAM_API_KEY)\"",
    "whinfo": "curl -s https://api.telegram.org/bot$(dotenv -e .env.local -p TELEGRAM_API_KEY)/getWebhookInfo | jq",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@geist-ui/react": "^2.1.5",
    "@geist-ui/react-icons": "^1.0.1",
    "@vercel/node": "^1.11.1",
    "fast-memoize": "^2.5.2",
    "mongo-cursor-pagination": "^7.4.0",
    "mongodb": "^3.6.10",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "react-telegram-login": "^1.1.2",
    "rehype-raw": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "swr": "^0.5.6",
    "telegram-text-entities-filler": "https://github.com/alwaysbegrowing/telegram-text-entities-filler",
    "time-until": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "dotenv-cli": "^4.0.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "javascript-time-ago": "^2.3.8",
    "ngrok": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "react-time-ago": "^7.0.0"
  }
}
